<h1>新規ユーザー登録</h1>

<%= form_with model: @user, local: true do |form| %>
  <div>
    <%= form.label :name, "名前" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :birthday, "誕生日" %>
    <%= form.date_select :birthday %>
  </div>

  <div>
    <%= form.submit "登録" %>
  </div>
<% end %>
